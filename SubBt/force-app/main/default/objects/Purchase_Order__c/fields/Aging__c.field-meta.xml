<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Aging__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF(  ISPICKVAL(Status__c, &apos;Approved&apos;),IF( Completion__c == 1 ,TODAY()  - Completion_Date__c + 1 , 1) , 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The number of days since the Schedule Item has been completed and the PO has not been Approved.</inlineHelpText>
    <label>Aging</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
