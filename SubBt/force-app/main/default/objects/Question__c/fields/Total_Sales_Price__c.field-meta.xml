<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Sales_Price__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF( Markup__c &gt; 0 , (Markup__c * (Quantity__c * (Cost__c +  Upgrade_Costs__c)) + (Quantity__c * (Cost__c +  Upgrade_Costs__c))), 

(Quantity__c * (Cost__c +  Upgrade_Costs__c)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Total Sales Price for your Customer.</inlineHelpText>
    <label>Total Sales Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
