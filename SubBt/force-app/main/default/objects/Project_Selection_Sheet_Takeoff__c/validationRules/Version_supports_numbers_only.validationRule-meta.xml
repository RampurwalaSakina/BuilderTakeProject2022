<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Version_supports_numbers_only</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(REGEX( Version_Number__c ,&quot;^[.0-9_]*$&quot;))</errorConditionFormula>
    <errorMessage>Enter a valid number</errorMessage>
</ValidationRule>
