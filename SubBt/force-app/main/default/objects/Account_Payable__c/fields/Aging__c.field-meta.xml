<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Aging__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF( NOT(ISPICKVAL( Status__c , &apos;Paid&apos;)) , Today() -  Date_Received__c , 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Number of days since the Bill was received and the Bill has not been paid.</inlineHelpText>
    <label>Aging</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
