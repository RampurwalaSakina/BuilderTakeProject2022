<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Net_Change_by_Change_Orders__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>(Change_Order_Additions__c + CO_Additions_Approved_This_Month__c) + (Change_Order_Decuctions__c + CO_Deductions_Approved_in_Previous_Month__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Sum of the Change Orders approved for this job.</inlineHelpText>
    <label>Net Change by Change Orders</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
